<div class="container">
  <div class="row mt-5">
    <h4>検索一覧</h4>
  </div>

  <div class="row">
    <%= link_to "新しい順", search_posts_path(latest: "true",keyword: params[:keyword]) %>
    |<%= link_to "古い順", search_posts_path(old: "true",keyword: params[:keyword]) %>
    |<%= link_to "評価の高い順", search_posts_path(star_count: "true",keyword: params[:keyword]) %>
  </div>

  <div class="row mt-3">
    <%= render 'public/posts/index', posts: @posts, fdivs: @fdivs %>
  </div>
</div>